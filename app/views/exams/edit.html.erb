<%= form_for @exam do |f| %>
        <%= f.label :tilte %>
        <%= f.text_field :title %>
        <br>
        <br>
    <%= f.fields_for :questions do |questions_form| %>
      <%= render 'question', f: questions_form %>
    <% end %>
    
  <%=f.submit  :class => 'btn btn-primary' %>
  <%= link_to t('.back', :default => t("helpers.links.back")),
               exams_path, :class => 'btn btn-default'  %>
<% end %>